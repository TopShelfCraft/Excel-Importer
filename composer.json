{
	"name": "topshelfcraft/spreadsheet-tools",
	"description": "A toolkit for reading, writing, and processing spreadsheet data in Craft CMS.",
	"type": "craft-plugin",
	"version": "3.0.0-beta.1",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"excel-importer",
		"Excel Importer",
		"spreadsheet",
		"spreadsheet-tools",
		"import",
		"excel",
		"XLS",
		"CSV"
	],
	"support": {
		"docs": "https://raw.githubusercontent.com/TopShelfCraft/Spreadsheet-Tools",
		"issues": "https://github.com/TopShelfCraft/Spreadsheet-Tools/issues"
	},
	"license": "proprietary",
	"authors": [
		{
			"name": "Top Shelf Craft (Michael Rog)",
			"homepage": "https://topshelfcraft.com"
		},
		{
			"name": "Aaron Waldon",
			"homepage": "https://www.causingeffect.com"
		},
		{
			"name": "TJ Draper",
			"homepage": "https://buzzingpixel.com"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"require": {
		"php": ">=7.0.0",
		"craftcms/cms": "^3.0.0",
		"symfony/filesystem": "^2.7 || ^3.0",
		"phpoffice/phpspreadsheet": "^1.1"
	},
	"autoload": {
		"psr-4": {
			"topshelfcraft\\spreadsheettools\\": "src/"
		}
	},
	"extra": {
		"name": "Spreadsheet Tools",
		"handle": "spreadsheet-tools",
		"schemaVersion": "0.0.0.0",
		"hasCpSettings": false,
		"hasCpSection": false,
		"changelogUrl": "https://raw.githubusercontent.com/TopShelfCraft/Spreadsheet-Tools/master/CHANGELOG.md",
		"class": "topshelfcraft\\spreadsheettools\\SpreadsheetTools",
		"components": {
			"helpers": "topshelfcraft\\spreadsheettools\\services\\HelperService",
			"spreadsheet": "topshelfcraft\\spreadsheettools\\services\\SpreadsheetService"
		}
	}
}
